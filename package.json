  "license": "BSD-3-Clause-Clear",
  "author": {
    "name": "Orange",
    "email": "khiops.team@orange.com"
  },
  "description": "The Logarithmic histogram library used into Khiops Visualization",
  "repository": {
    "type": "git",
    "url": "https://github.com/KhiopsML/ngx-khiops-histogram.git"
  },
  "bugs": {
    "url": "https://github.com/KhiopsML/ngx-khiops-histogram/issues"
  },
  "homepage": "https://github.com/KhiopsML/ngx-khiops-histogram#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-lib": "ng build ngx-khiops-histogram",
    "pack-slib": "cd dist/ngx-khiops-histogram && npm pack",
    "watch:lib": "ng build ngx-khiops-histogram --watch",
    "pack:lib": "cd projects/ngx-khiops-histogram && npm pack",
    "watch:pack:lib": "npx nodemon --watch projects/ngx-khiops-histogram/src --exec \"npm run pack:lib\"",
    "dev": "concurrently \"npm run start\" \"npm run watch:lib\" \"npm run watch:pack:lib\"",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "setTag": "./scripts/tag.sh"
  },
  "dependencies": {
    "@angular/animations": "^20.3.7",
    "@angular/common": "^21.0.6",
    "@angular/compiler": "^20.3.15",
    "@angular/core": "^20.3.7",
    "@angular/forms": "^20.3.7",
    "@angular/platform-browser": "^20.3.7",
    "@angular/platform-browser-dynamic": "^20.3.7",
    "@angular/router": "^20.3.7",
    "ngx-khiops-histogram": "file:dist/ngx-khiops-histogram",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.7",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^20.3.15",
    "@types/jasmine": "~5.1.12",
    "jasmine-core": "~5.13.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
    "typescript": "~5.9.3"
  }
}
