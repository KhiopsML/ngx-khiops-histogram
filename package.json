    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3",
    "ngx-khiops-histogram": ".\\dist\\ngx-khiops-histogram"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.4",
